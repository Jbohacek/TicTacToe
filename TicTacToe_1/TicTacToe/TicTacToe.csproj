<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>Ikonka.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Ikonka.ico" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Obrazky.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Obrazky.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Obrazky.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Obrazky.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>